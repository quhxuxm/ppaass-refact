    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ppaass-proxy'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ppaass-proxy",
                    "--package=proxy"
                ],
                "filter": {
                    "name": "ppaass-proxy",
                    "kind": "bin"
                }
            },
            "args": [
                "-p",
                "90"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ppaass-agent'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ppaass-agent",
                    "--package=agent"
                ],
                "filter": {
                    "name": "ppaass-agent",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}